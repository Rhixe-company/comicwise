{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "ğŸ“¦ Install Dependencies",
      "type": "shell",
      "command": "pnpm",
      "args": ["install"],
      "group": {
        "kind": "none"
      },
      "presentation": {
        "reveal": "always",
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true,
        "group": "setup"
      },
      "problemMatcher": []
    },
    {
      "label": "ğŸš€ Dev Server",
      "type": "shell",
      "command": "pnpm",
      "args": ["dev"],
      "isBackground": true,
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": {
        "pattern": {
          "regexp": "^.*$"
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^.*Local:.*",
          "endsPattern": "^.*ready - started server.*"
        }
      },
      "presentation": {
        "reveal": "always",
        "panel": "dedicated",
        "showReuseMessage": false,
        "clear": false,
        "group": "development"
      }
    },
