{
  "compounds": [
    {
      "name": "Full Stack Dev",
      "configurations": ["Next: Dev Server", "Attach to Process"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 1
      },
      "stopAll": true
    },
    {
      "name": "Dev + Debug",
      "configurations": ["Next: Dev with Inspect", "Attach to Next (9229)"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 2
      },
      "stopAll": true
    },
    {
      "name": "Full Test Suite",
      "configurations": ["Vitest Unit Tests", "Playwright E2E"],
      "presentation": {
        "hidden": false,
        "group": "testing",
        "order": 1
      },
      "stopAll": true
    },
    {
      "name": "Docker + Debug",
      "configurations": ["Docker: Dev Environment", "Attach to Docker Node"],
      "presentation": {
        "hidden": false,
        "group": "docker",
        "order": 1
      },
      "stopAll": true
    }
  ],
  "configurations": [
    {
      "name": "Next: Dev Server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev"],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "env": {
        "NODE_OPTIONS": "--enable-source-maps",
        "NODE_ENV": "development"
      },
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 1
      },
      "serverReadyAction": {
        "pattern": "started server on .+, url: (https?://.+)",
        "uriFormat": "%s",
        "action": "debugWithChrome"
      }
    },
    {
      "name": "Next: Dev with Inspect",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev:debug"],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "port": 9229,
      "env": {
        "NODE_OPTIONS": "--inspect=9229 --enable-source-maps"
      },
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 2
      }
    },
    {
      "name": "Attach to Next (9229)",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 3
      }
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 4
      }
    },
    {
      "name": "Next: Build",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["build"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "build",
        "order": 1
      }
    },
    {
      "name": "Next: Start Production",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start"],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "production"
      },
      "presentation": {
        "hidden": false,
        "group": "production",
        "order": 1
      }
    },
    {
      "name": "Vitest Unit Tests",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["test:unit"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "testing",
        "order": 1
      }
    },
    {
      "name": "Vitest Current File",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["test:unit", "${file}"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "testing",
        "order": 2
      }
    },
    {
      "name": "Playwright E2E",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["test"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "testing",
        "order": 3
      }
    },
    {
      "name": "Playwright UI Mode",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["test:ui"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "testing",
        "order": 4
      }
    },
    {
      "name": "Database: Seed",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["db:seed"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "database",
        "order": 1
      }
    },
    {
      "name": "Database: Studio",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["db:studio"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "database",
        "order": 2
      }
    },
    {
      "name": "Docker: Dev Environment",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["docker:up"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "docker",
        "order": 1
      }
    },
    {
      "name": "Attach to Docker Node",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/app",
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "docker",
        "order": 2
      }
    },
    {
      "name": "TypeScript: Watch",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["type-check:watch"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 5
      }
    },
    {
      "name": "ESLint: Fix All",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["lint:fix"],
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "linting",
        "order": 1
      }
    }
  ],
  "version": "0.2.0"
}
