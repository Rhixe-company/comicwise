    },
    {
      "name": "üîß Dev + Debug + TypeScript Watch",
      "configurations": [
        "Next: Dev with Inspect",
        "Attach to Next (9229)",
        "TypeScript: Watch"
      ],
      "presentation": {
        "hidden": false,
        "group": "‚≠ê favorites",
        "order": 2
      },
      "stopAll": true
    },
    {
      "name": "üß™ Full Test Suite",
      "configurations": ["Vitest Unit Tests", "Playwright E2E"],
      "presentation": {
        "hidden": false,
        "group": "testing",
        "order": 1
      },
      "stopAll": true,
      "preLaunchTask": "‚úÖ Validate All"
    },
    {
      "name": "üê≥ Docker Dev Environment + Debug",
      "configurations": ["Docker: Dev Environment", "Attach to Docker Node"],
      "presentation": {
        "hidden": false,
        "group": "docker",
        "order": 1
      },
      "stopAll": true
    },
    {
      "name": "‚ö° Complete CI Pipeline",
      "configurations": [
        "üîç Type Check",
        "üé® Lint",
        "Vitest Unit Tests",
        "Next: Build"
      ],
      "presentation": {
        "hidden": false,
        "group": "ci",
        "order": 1
      },
      "stopAll": false
    }
  ],
  "configurations": [
    {
      "name": "Next: Dev Server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev"],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "env": {
        "NODE_OPTIONS": "--enable-source-maps --max-old-space-size=8192 --trace-warnings",
        "NODE_ENV": "development",
        "DEBUG": "next:*"
      },
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 1
      },
      "serverReadyAction": {
        "pattern": "started server on .+, url: (https?://.+)",
        "uriFormat": "%s",
        "action": "debugWithChrome"
      }
    },
    {
      "name": "Next: Dev with Inspect",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev:debug"],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "port": 9229,
      "env": {
        "NODE_OPTIONS": "--inspect=9229 --enable-source-maps --max-old-space-size=8192 --trace-warnings"
      },
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 2
      },
      "restart": true,
      "sourceMaps": true
    },
    {
      "name": "Attach to Next (9229)",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 3
      }
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "presentation": {
        "hidden": false,
        "group": "development",
        "order": 4
      }
    },
    {
      "name": "Next: Build",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["build"],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "production",
        "NODE_OPTIONS": "--max-old-space-size=6144"
      },
      "presentation": {
        "hidden": false,
        "group": "build",
        "order": 1
      },
      "preLaunchTask": "üßπ Clean"
    },
    {
      "name": "Next: Build with Analysis",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["build:analyze"],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "production",
        "ANALYZE": "true"
      },
      "presentation": {
        "hidden": false,
        "group": "build",
        "order": 2
      }
    },
    {
      "name": "Next: Start Production",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start"],
      "console": "integratedTerminal",
