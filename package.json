{
  "name": "comicwise",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "pnpm clean:cache",
    "build": "next build",
    "postbuild": "pnpm dlx next-sitemap --config nextSitemap.config.ts",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:debug": "next build --debug",
    "build:standalone": "cross-env NEXT_OUTPUT_MODE=standalone next build",
    "cache:clear": "tsx --env-file=.env.local scripts/clearCache.ts",
    "cache:stats": "tsx --env-file=.env.local scripts/cacheStats.ts",
    "check-updates": "pnpm dlx npm-check-updates --deep -i && pnpm dlx update-browserslist-db@latest",
    "ci": "pnpm validate && pnpm test:unit:run",
    "ci:build": "pnpm build",
    "ci:full": "pnpm validate && pnpm test:unit:run && pnpm test && pnpm build",
    "ci:lint": "pnpm lint:strict",
    "ci:test": "pnpm test --reporter=json --reporter=html",
    "clean": "rimraf .next out dist build .turbo coverage .react-email",
    "clean:all": "pnpm clean && rimraf node_modules pnpm-lock.yaml",
    "clean:cache": "rimraf .next/cache",
    "cleanup": "tsx scripts/projectCleanup2025.ts",
    "cleanup:dry-run": "tsx scripts/projectCleanup2025.ts --dry-run",
    "cw": "tsx scripts/cli/cw.ts",
    "db:check": "drizzle-kit check --config drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:pull": "drizzle-kit introspect --config drizzle.config.ts",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "db:reset": "pnpm db:drop && pnpm db:push && pnpm db:seed",
    "db:reset:hard": "pnpm db:drop && pnpm db:generate && pnpm db:push && pnpm db:seed",
    "db:seed": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --all",
    "db:seed:chapters": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --chapters",
    "db:seed:comics": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --comics",
    "db:seed:dry-run": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --dry-run",
    "db:seed:users": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --users",
    "db:seed:verbose": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --verbose",
    "db:studio": "drizzle-kit studio",
    "deploy:preview": "vercel",
    "deploy:vercel": "vercel --prod",
    "predev": "pnpm clean:cache",
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:https": "next dev --experimental-https",
    "docker:build": "docker compose build",
    "docker:clean": "docker compose down -v --remove-orphans",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:up": "docker compose up -d",
    "format": "prettier --config .prettierrc.ts --write \"**/*.{ts,tsx,js,jsx,mjs,mts,cjs,cts,json,css,md}\"",
    "format:check": "prettier --config .prettierrc.ts --check \"**/*.{ts,tsx,js,jsx,mjs,mts,cjs,cts,json,css,md}\"",
    "health:all": "pnpm health:db && pnpm health:redis && pnpm health:check",
    "health:check": "tsx --env-file=.env.local scripts/healthCheck.ts",
    "health:db": "tsx --env-file=.env.local scripts/checkDb.ts",
    "health:redis": "tsx --env-file=.env.local scripts/checkRedis.ts",
    "imports:check": "tsx scripts/replaceImportsEnhanced.ts --dry-run --verbose",
    "imports:optimize": "tsx scripts/replaceImportsEnhanced.ts",
    "postinstall": "drizzle-kit generate --config drizzle.config.ts || echo 'Drizzle not configured'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings=0",
    "optimize:all": "tsx scripts/comprehensiveMasterOptimization2025.ts",
    "optimize:camelcase": "tsx scripts/camelCaseConverter2025.ts",
    "optimize:types": "tsx scripts/updateAnyTypes.ts",
    "predeploy": "pnpm validate && pnpm build",
    "prepare": "husky",
    "priority": "tsx scripts/prioritySystem.ts",
    "queue:clean": "tsx --env-file=.env.local scripts/queueWorker.ts --clean",
    "queue:stats": "tsx --env-file=.env.local scripts/queueWorker.ts --stats",
    "queue:worker": "tsx --env-file=.env.local scripts/queueWorker.ts",
    "redis:cli": "docker exec -it comicwise-redis redis-cli",
    "redis:flush": "docker exec -it comicwise-redis redis-cli FLUSHALL",
    "scaffold": "tsx scripts/scaffold.ts",
    "scaffold:action": "tsx scripts/scaffold.ts --type=action",
    "scaffold:component": "tsx scripts/scaffold.ts --type=component",
    "scaffold:hook": "tsx scripts/scaffold.ts --type=hook",
    "seed:chapters": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --chapters",
    "seed:clear": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --clear",
    "seed:comics": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --comics",
    "seed:users": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --users",
    "seed:validate": "tsx --env-file=.env.local src/database/seed/runEnhanced.ts --dry-run --verbose",
    "setup": "pnpm install && pnpm db:push && pnpm db:seed",
    "setup:clean": "pnpm clean && pnpm install && pnpm db:reset",
    "setup:full": "pnpm clean:all && pnpm install && pnpm db:reset && pnpm build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "test": "playwright test",
    "test:all": "pnpm test:unit:run && pnpm test",
    "test:codegen": "playwright codegen",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "test:ui": "playwright test --ui",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:run": "vitest run",
    "test:unit:watch": "vitest watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "typegen": "next typegen",
    "upload:bulk": "tsx --env-file=.env.local scripts/uploadBulk.ts",
    "upload:bulk:aws": "tsx --env-file=.env.local scripts/uploadBulk.ts --provider=aws",
    "upload:bulk:cloudinary": "tsx --env-file=.env.local scripts/uploadBulk.ts --provider=cloudinary",
    "upload:bulk:dry-run": "tsx --env-file=.env.local scripts/uploadBulk.ts --dry-run",
    "upload:bulk:imagekit": "tsx --env-file=.env.local scripts/uploadBulk.ts --provider=imagekit",
    "upload:test": "tsx --env-file=.env.local scripts/uploadBulk.ts --test",
    "upstash": "pnpm dlx @upstash/qstash-cli dev",
    "validate": "pnpm type-check && pnpm lint:strict && pnpm format:check",
    "validate:quick": "pnpm type-check && pnpm lint"
  },
  "lint-staged": {
    "**/*": [
      "pnpm format"
    ]
  },
